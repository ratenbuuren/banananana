<UserControl x:Class="Banananana.TaskPile"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Banananana"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="400">
    <Grid>
        <StackPanel x:Name="stackPanel" Margin="0">
            <Label x:Name="label" Content="Title" Background="#FF7EDEDE" FontSize="18.667"/>
        </StackPanel>

    </Grid>
</UserControl>
